package io.vertigo.orchestra.domain.execution

create DtDefinition DT_O_ACTIVITY_WORKSPACE {
	storeName : "orchestra"
	id acwId			{domain : DO_O_IDENTIFIANT, label :"Id de l'execution d'un processus", }
	field isIn  		{domain : DO_O_BOOLEEN, label :"Workspace in/out", required:"true",}
	field workspace 	{domain : DO_O_JSON_TEXT, label :"Contenu du workspace", required:"false",}
  
},



create Association A_TKW_TKE {
  fkFieldName : "aceId"
    
 	dtDefinitionA : DT_O_ACTIVITY_WORKSPACE
	dtDefinitionB : DT_O_ACTIVITY_EXECUTION
			
	navigabilityA : "false"
	navigabilityB : "true"
	
	multiplicityA:"0..*"
	multiplicityB:"1..1"
	
	labelA : "ActivityWorkspace"
	labelB : "ActivityExecution"
	roleA : "ActivityWorkspace"
	roleB : "ActivityExecution"
			
}


