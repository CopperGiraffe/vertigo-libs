package io.vertigo.orchestra.dao.node

create Task TK_INSERT_NODE {
	storeName : "orchestra"
	className : "io.vertigo.dynamox.task.TaskEngineProc"
	request : "
		INSERT INTO O_NODE 
			(NOD_ID, LAST_HEARTBEAT)
		values  
			(#NODE.NOD_ID#, #NODE.LAST_HEARTBEAT#)
		"
		
	attribute NODE	{domain : DO_DT_O_NODE_DTO  notNull:"true" 	inOut :"in"}
}
