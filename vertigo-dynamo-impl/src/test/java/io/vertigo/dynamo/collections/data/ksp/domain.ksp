package io.vertigo.dynamo.collections.data.domain

create Formatter FmtDefault{
	className : "io.vertigo.dynamox.domain.formatter.FormatterDefault"
}

create Domain DoId{
	dataType : Long
	formatter : FmtDefault
}

create Domain DoText {
	dataType : String
	formatter : FmtDefault
}

create Domain DoKeyword {
	dataType : String
	formatter : FmtDefault
}

create Domain DoInteger {
	dataType : Integer
	formatter : FmtDefault
}

