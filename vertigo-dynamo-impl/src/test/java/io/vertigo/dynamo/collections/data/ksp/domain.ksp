}

create Domain DO_STRING {
	dataType : String
	formatter : FMT_DEFAULT
}

create Domain DO_LONG {
	dataType : Long
}

create Domain DO_FULL_TEXT {
	dataType : String
	formatter : FMT_DEFAULT
}

create Domain DO_KEYWORD {
	dataType : String
	formatter : FMT_DEFAULT
}

create Domain DO_CONSO {
	dataType : BigDecimal
	formatter : FMT_DEFAULT
}

