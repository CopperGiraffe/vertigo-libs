<?xml version =	'1.0' encoding = 'ISO-8859-1'?>
<config>
	<module name="vertigo-dynamo">
		<component api="KTransactionManager" class="io.vertigo.dynamo.impl.transaction.KTransactionManagerImpl" />
		<component api="KVDataStoreManager" class="io.vertigo.dynamo.impl.kvdatastore.KVDataStoreManagerImpl">
			<plugin class="io.vertigo.dynamo.plugins.kvdatastore.berkeley.BerkeleyKVDataStorePlugin">
				<param name="dataStoreName" value="default" />
				<param name="fileName" value="d:/dynamo-tmp" />
				<param name="inMemory" value="true" />
			</plugin>
		</component>
	</module>
</config>