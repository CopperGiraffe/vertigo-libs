package io.vertigo.dynamo.file.data.domain

create FileInfo FI_FILE_INFO_STD {
	storeName : "main"
}

create DtDefinition DT_VX_FILE_INFO {
	 id  filId 			{domain : DO_URI 	label : "Identifiant" }
	 field fileName 		{domain : DO_LABEL 	label : "Nom" 					required : "true" }
 	 field length 			{domain : DO_SIZE 	label : "Taille" 				required : "true" }
	 field lastModified 	{domain : DO_INSTANT	label : "Date de modification" 	required : "true" }
	 field fileData 		{domain : DO_LABEL	label : "data" 					required : "false", persistent:"false" }	 
}