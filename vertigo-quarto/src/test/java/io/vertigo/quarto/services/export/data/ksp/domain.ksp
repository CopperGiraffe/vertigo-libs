package io.vertigo.quarto.data.domain


create Formatter FmtBooleanDefault {
	className : "io.vertigo.dynamox.domain.formatter.FormatterBoolean"
	args : "Yes; No"
}

create Formatter FmtDefault{
	className : "io.vertigo.dynamox.domain.formatter.FormatterDefault"
}

create Domain DoString {
	dataType : String
	formatter : FmtDefault
}

create Domain DoId {
	dataType : Long
	formatter : FmtDefault
}

create Domain DoBoolean {
	dataType : Boolean
	formatter : FmtDefault
}
