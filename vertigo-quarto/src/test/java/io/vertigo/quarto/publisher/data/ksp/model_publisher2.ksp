	field prenom 		{ domain : DoString      				label : "Prénom"      						cardinality:"?"}
	field address 		{ domain : DoString      				label : "Adresse {multi ligne}"      		cardinality:"?"}
	field commentaire 	{ domain : DoString      				label : "Commentaire {multi paragraphe}" 	cardinality:"?"}
	field boolean1 		{ domain : DoBoolean     				label : "booleen 1"      					cardinality:"?"}
	field boolean2 		{ domain : DoBoolean     				label : "booleen 2"      					cardinality:"?"}
	field boolean3 		{ domain : DoBoolean     				label : "booleen 3"      					cardinality:"?"}
	field testDummy 	{ domain : DoString    				label : "Test champs inutilisé"     		cardinality:"?"}
	field testLong	 	{ domain : DoLong      				label : "Test long"      					cardinality:"?"}
	field testDouble 	{ domain : DoDouble      				label : "Test double"      					cardinality:"?"}
	field testInteger 	{ domain : DoInteger      				label : "Test integer"      				cardinality:"?"}
	field testDate	 	{ domain : DoDate      				label : "Test date"      					cardinality:"?"}
	field dtcTest 		{ domain : DoDtPublisherMock 	label : "Test list"      					cardinality:"*"}
