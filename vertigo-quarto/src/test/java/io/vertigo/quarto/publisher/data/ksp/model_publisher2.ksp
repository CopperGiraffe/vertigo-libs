package io.vertigo.publisher.mock  

/*Pour chargement des données*/
create DtDefinition DtVille {
  	field nom 			{domain : DoString      	label : "Nom"      		cardinality:"?"}
  	field codePostal 	{domain : DoString      	label : "Code postal" 	cardinality:"?"}
}
 
create DtDefinition DtAddress {
  	field rue			{domain : DoString      	label : "Rue"      		cardinality:"?"}
  	field ville 		{domain : DoDtVille   label : "Ville"      	cardinality:"?"}
}
 
create DtDefinition DtEnqueteur {
  	field nom 			{domain : DoString      	label : "Nom"      		cardinality:"?"}
  	field prenom 		{domain : DoString      	label : "Prenom"      	cardinality:"?"}
}
 
create DtDefinition DtMisEnCause {
  	field siHomme 			{domain : DoBoolean      	label : "Sexe"      		cardinality:"?"}
  	field nom 				{domain : DoString      	label : "Nom"      			cardinality:"?"}
  	field prenom 			{domain : DoString      	label : "Prenom"      		cardinality:"?"}
  	field adressesConnues 	{domain : DoDtAddress	label : "Addresses connues"	cardinality:"*"}   
}

create DtDefinition DtEnquete {
  	field enqueteTerminee {domain : DoBoolean      			label : "Terminée?" cardinality:"?"}
  	field codeEnquete 		{domain : DoString      			label : "Code"      cardinality:"?"}
  	field fait 				{domain : DoString      			label : "Sexe"      cardinality:"?"}
  	field siGrave 			{domain : DoBoolean      			label : "Sexe"      cardinality:"?"}
}

create DtDefinition DtPublisherMock {
	field titre 		{ domain : DoString      				label : "Titre"      						cardinality:"?"}
