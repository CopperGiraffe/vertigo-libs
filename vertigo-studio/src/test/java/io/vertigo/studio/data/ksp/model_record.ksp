package io.vertigo.studio.domain.security


create DtDefinition DT_RECORD {
	stereotype : "KeyConcept"
	id	  dosId				{domain: DO_ID 		label: "Id"}
	
	field regId 				{domain: DO_ID label: "Region" required:"false" }
  	field depId 				{domain: DO_ID label: "Departement" required:"false" }
  	field comId 				{domain: DO_ID label: "Commune" required:"false" }
  
  	field typId 				{domain: DO_ID label: "Record type" 	required:"true"}
	field title 				{domain: DO_STRING label: "Title" 	required:"true"}
	field amount 				{domain: DO_CONSO label: "Amount" 	required:"true"}
	field utiIdOwner 			{domain: DO_ID label: "Owner" required:"true" }
  
  	field etaCd 				{domain: DO_STRING label: "State" 	required:"true"}	
}
